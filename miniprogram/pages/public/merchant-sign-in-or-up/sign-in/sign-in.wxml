<view class='head'>
  <image src="../../../../assets/homeImages/ganfanbao.png" catchtap="mine" mode="aspectFill"></image>
  <text>{{textValue}}</text>

  <van-cell-group style="width: 600rpx;">
    <van-field model:value="{{ phoneNumber }}" clearable type="number" label="手机号" placeholder="请输入手机号"/>
  </van-cell-group>

  <van-cell-group style="width: 600rpx;">
    <van-field model:value="{{ password }}" maxlength="11" password="{{password_state_type}}" label="密码" clearable placeholder="请输入密码" icon="{{password_state_icon}}" bind:click-icon="onClickIcon" />
  </van-cell-group>
  <view class="button-container">
    <button hover-class="button_hover" bindtap='login'>登录</button>
    <button hover-class="button_hover" bindtap='register'>注册</button>
  </view>
</view>
<view class="copyright">
  @FashionGod
</view>